<h1>New User</h1>
<form method="post" action="/users/user/new">
  <label>First Name</label>
  <input name="first_name" type="text" placeholder="Hank"></input>
  <br>
  <label>Last Name</label>
  <input name="last_name" type="text" placeholder="Hill"></input>
  <br>
  <label>Email</label>
  <input name="email" type="text" placeholder="hankhill@propane.biz"></input>
  <br>
  <label>Phone</label>
  <input name="phone" type="text" placeholder="(555) 555-5555"></input>
  <br>
  <input type="submit" value="Submit"></input>
</form>
