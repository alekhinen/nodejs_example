<a class="btn btn-success" href="/users/new">New</a>
<h1>Users List</h1>
{{message}}
<table class="table table-striped">
  <thead>
    <tr>
      <th></th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Date Created</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
      <tr>
        <td><a href="/users/user/{{_id}}">view</a></td>
        <td>{{name.first}}</td>
        <td>{{name.last}}</td>
        <td>{{email}}</td>
        <td>{{phone}}</td>
        <td>{{created}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
